{
	"dataset": {
		"name": "Capgmyo",
		"train_generator": {
			"trials": [1, 3, 4, 6, 8, 10],
			"batch_size":1024,
			"sample_weight": true,
			"dim": [8,16,3],
			"window_size": 3,
			"window_step": 1,
			"snr_db":[25,26],			
			"shuffle": true
		},
		"valid_generator": {
			"trials": [2,5,9],
			"batch_size": 1,
			"sample_weight": false,
			"dim": [8,16,3],
			"window_size": 3,
			"window_step": 1,
			"shuffle": false,
			"update_after_epoch": false
		},
		"test_generator": {
			"trials": [7],
			"batch_size": 1,
			"sample_weight": false,
			"dim": [8,16,3],
			"window_size": 3,
			"window_step": 1,
			"shuffle": false,
			"update_after_epoch": false
		}
	},

	"model": {
		"name": "VGG19_Capgmyo",
		"save": true,
		"save_file": "Capgmyo_vgg19",
		"extra": {
			"dropout_rate": 0.5,
			"dense":512,
			"batch_norm": true
		}
	},

	"training": {
		"optimizer": "SGD",
		"fine_tuning_epochs": 7,
		"training_epochs":35,
		"fine_tuning_lrate": 0.0001,
		"training_lrate":0.001
	},

	"logging": {
		"enable": true,
		"log_file": "Capgmyo_vgg19"
	}

	
}