{
	"dataset": {
		"name": "DB1",
		"train_generator": {
			"repetitions": [1, 3, 4, 6,  8, 10],
			"batch_size":550,
			"sample_weight": true,
			"dim": [15,10,3],
			"window_size": 15,
			"window_step": 5,
			"snr_db":[25,26],
			"rotation":2,
			"size_factor":2,	
			"shuffle": true
		},
		"valid_generator": {
			"repetitions": [2, 5, 9],
			"batch_size": 1,
			"sample_weight": false,
			"dim": [15,10,3],
			"window_size": 15,
			"window_step": 15,
			"shuffle": false,
			"update_after_epoch": false
		},
		"test_generator": {
			"repetitions": [7],
			"batch_size": 1,
			"sample_weight": false,
			"dim": [15,10,3],
			"window_size": 15,
			"window_step": 15,
			"partb_zero":false,
			"zero_column":0,
			"partb_noise":true,
			"noise_column":0,
			"shuffle": false,
			"update_after_epoch": false
		}
	},

	"model": {
		"name": "VGG16_NinaPro",
		"save_file": "DB1_vgg16_b2",
		"extra": {
			"dropout_rate":0.5,
			"dense1":512,
			"dense2":512,
			"dense3":128,
			"batch_norm": true
		}
	},

	"training": {
		"optimizer": "SGD",
		"fine_tuning_epochs": 18,
		"training_epochs":35,
		"fine_tuning_lrate": 0.0001,
		"training_lrate":0.01
	},

	"logging": {
		"enable": true,
		"log_file": "DB1_vgg16_b2"
	}

	
}