{
	"dataset": {
		"name": "Capgmyo",
		"train_generator": {
			"trials": [1, 3, 4, 6, 8, 10],
			"batch_size":1024,
			"sample_weight": true,
			"dim": [8,16,3],
			"window_size": 3,
			"window_step": 1,
			"shuffle": true
		},
		"valid_generator": {
			"trials": [2,5,9],
			"batch_size": 1,
			"sample_weight": false,
			"dim": [8,16,3],
			"window_size": 3,
			"window_step": 1,
			"shuffle": false,
			"update_after_epoch": false
		},
		"test_generator": {
			"trials": [7],
			"batch_size": 1,
			"sample_weight": false,
			"dim": [8,16,3],
			"window_size": 3,
			"window_step": 1,
			"partb_zero":false,
			"zero_column1":0,
			"zero_column2":0,
			"partb_noise":true,
			"noise_column1":0,
			"noise_column2":15,			
			"shuffle": false,
			"update_after_epoch": false
		}
	},

	"model": {
		"name": "Inceptionv3_Capgmyo",
		"save": true,
		"save_file": "Capgmyo_inceptionv3_b2",
		"extra": {
			"dropout_rate": 0.5,
			"dense1":128,
			"dense2":128,
			"batch_norm": true
		}
	},

	"training": {
		"optimizer": "SGD",
		"fine_tuning_epochs": 15,
		"training_epochs":15,
		"fine_tuning_lrate": 0.0001,
		"training_lrate":0.01
	},

	"logging": {
		"enable": true,
		"log_file": "Capgmyo_inceptionv3_b2"
	}

	
}